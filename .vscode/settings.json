{
  "dart.flutterSdkPath": "super_app/.fvm/flutter_sdk",
  // Remove .fvm files from search
  "search.exclude": {
    "**/.fvm": true
  },
  // Remove from file watching
  "files.watcherExclude": {
    "**/.fvm": true
  },
  "dart.lineLength": 120,
  "[dart]": {
    "editor.rulers": [
      120
    ],
  },
  "files.autoSave": "onFocusChange",
  "editor.fontFamily": "Sans Serif, Menlo, Monaco, 'Courier New', monospace",
}